<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Comments</title>


    {% block builtin_head_bundles %}
    <link href='https://fonts.googleapis.com/css?family=Lato:100,200,300,400,700' rel='stylesheet' type='text/css'>
    {{ assets.definedBundle("alwaysHeadStylesheets") }}
    {{ assets.definedBundle("commentsAdminStylesheets") }}
    {% endblock %}
    {% block extra_head %}
    {% endblock %}

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
{% block body_content %}
<div class="custom-wrapper pure-g" id="menu">
    <div class="pure-u-1 pure-u-md-1-3">
        <div class="pure-menu">
            <a href="#" class="pure-menu-heading custom-brand"><img src="{{ assets.resource('stallion-rearing-red-299.png', 'stallion') }}"> <span>Stallion Admin</span></a>
            <a href="#" class="custom-toggle" id="toggle"><s class="bar"></s><s class="bar"></s></a>
        </div>
    </div>
    <div class="pure-u-1 pure-u-md-1-3">
        <div class="pure-menu pure-menu-horizontal custom-can-transform">
        </div>
    </div>
    <div class="pure-u-1 pure-u-md-1-3">
        <div class="pure-menu pure-menu-horizontal custom-menu-3 custom-can-transform">
            <ul class="pure-menu-list">
            </ul>
        </div>
    </div>
</div>
<div id="main-column-outer">
    <div class="pure-g">
        <div class="pure-u-1">
          <h5>Comments management</h5>
          <comment-edit-form></comment-edit-form>
          <comments-table></comments-table>
        </div>
    </div>
</div>
{% endblock %}
{% block builtin_footer_bundles %}
{{ assets.definedBundle("alwaysFooterJavascripts") }}
{{ assets.definedBundle("commentsAdminJavascripts") }}
<script src="{{ assets.resource('comments-manage-riot.tag', 'flatBlog') }}" type="riot/tag"></script>
{% endblock %}
{% block pre_end_body %}
{% endblock %}
</body>
</html>




