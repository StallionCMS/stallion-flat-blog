{% extends "stallion:public/semipublic-base.jinja" %}
{% block extra_head %}
<link href="{{ assets.resource('my-subscriptions.css', 'flatBlog') }}" rel="stylesheet" />
{% endblock %}
{% block main_column %}
<div id="st-comments-dashboard-content" class="container">
  <div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-8">
      <div id="subscriptions-table-wrap">
            <my-subscriptions></my-subscriptions>
      </div>
    </div>
    <div class="col-md-2">
    </div>
  </div><!--end row-->
</div>
{% endblock %}

{% block pre_end_body %}
<script type="text/javascript">
  window.st_contacts_context = {
      secretToken: {{ secretToken }},
      subscriptions: {{ subscriptionsSafeJson }},
      contact: {{ contact }}
  }
</script>
<script src="{{ assets.resource('my-subscriptions-riot.tag?processor=riot', 'flatBlog') }}" type="text/javascript"></script>
<script type="text/javascript">
  $(document).ready(function() {
    riot.compile(function() {
      stMySubscriptions.init();
    });
  });
</script>
{% endblock %}

